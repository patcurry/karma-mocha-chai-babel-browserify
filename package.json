{
  "name": "karma-mocha-chai-babel-browserify",
  "version": "0.0.1",
  "description": "This is a front end javascript unit testing set up",
  "main": "index.js",
  "scripts": {
    "build": "browserify src/smoke.js -t babelify -o dist/smoke.js",
    "dev": "watchify src/smoke.js -t babelify -o dist/smoke.js",
    "test-mocha-karma": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patcurry/karma-mocha-chai-babel-browserify.git"
  },
  "author": "Pat Curry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patcurry/karma-mocha-chai-babel-browserify/issues"
  },
  "homepage": "https://github.com/patcurry/karma-mocha-chai-babel-browserify#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.4.2"
  },
  "devDependencies": {
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-opera-launcher": "^1.0.0",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
